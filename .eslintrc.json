{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "object-curly-newline": 0
  }
}
